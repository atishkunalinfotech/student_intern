<div class="faq">
  <div class="container">
    <div class="row" style="margin-top:150px;">
      <div class="span12">
       <p>
          <%= link_to 'New City', new_admin_city_path %>
       </p>
<p id="notice"><%= notice %></p>

<h1>Listing Admin Cities</h1>

<table width="100%" border="0" class="table table-bordered table-striped">
  <thead>
    <tr>
      <th>City name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @admin_cities.each do |admin_city| %>
      <tr>
        <td><%= admin_city.city_name %></td>
        <td><%= link_to 'Show', admin_city %></td>
        <td><%= link_to 'Edit', edit_admin_city_path(admin_city) %></td>
        <td><%= link_to 'Destroy', admin_city, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
</div>
    </div>
  </div>
</div>


